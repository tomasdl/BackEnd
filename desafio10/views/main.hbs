<h2 class="hola">Lista de Productos</h2>
{{#if listExists}}
<table class="table">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Foto</th>
    </tr>
  </thead>
  <tbody>
{{#each elementos}}
    <tr>
        <td class="titulo">{{this.title}}</td>
        <td class="precio">{{this.price}}</td>
        <td ><img src="{{this.thumbnail}}" alt="alternatetext" class="imagen"></td>
    </tr>
{{/each}}
  </tbody>
</table>
{{else}}
<h3>No hay Productos</h3>
{{/if}}
